style "scrollbar" {

  engine "pixmap" {

    image {
      function = BOX
      detail = "trough"
      file = "progress-slider/trough-scrollbar.png"
      border = { 2, 2, 3, 3 }
      stretch = TRUE
      orientation = HORIZONTAL
    }

    image {
      function = BOX
      detail = "trough"
      file = "progress-slider/trough-scrollbar.png"
      border = { 3, 3, 2, 2 }
      stretch = TRUE
      orientation = VERTICAL
    }

    image {
      function = ARROW
      overlay_file = "frame/null.png"
      overlay_border = { 0, 0, 0, 0 }
      overlay_stretch = FALSE
      arrow_direction = UP
    }

    image {
      function = ARROW
      overlay_file = "frame/null.png"
      overlay_border = { 0, 0, 0, 0 }
      overlay_stretch = FALSE
      arrow_direction = DOWN
    }

    image {
      function = ARROW
      overlay_file = "frame/null.png"
      overlay_border = { 0, 0, 0, 0 }
      overlay_stretch = FALSE
      arrow_direction = LEFT
    }

    image {
      function = ARROW
      overlay_file = "frame/null.png"
      overlay_border = { 0, 0, 0, 0 }
      overlay_stretch = FALSE
      arrow_direction = RIGHT
    }

    # Sliders

    image {
      function = SLIDER
      state = NORMAL
      file = "progress-slider/slider-horiz.png" 
      border = { 5, 5, 3, 3 }
      stretch = TRUE
      orientation = HORIZONTAL
    }

    image {
      function = SLIDER
      state = ACTIVE
      file = "progress-slider/slider-horiz-active.png" 
      border = { 5, 5, 3, 3 }
      stretch = TRUE
      orientation = HORIZONTAL
    }

    image {
      function = SLIDER
      state = PRELIGHT
      file = "progress-slider/slider-horiz-prelight.png" 
      border = { 5, 5, 3, 3 }
      stretch = TRUE
      orientation = HORIZONTAL
    }

    image {
      function = SLIDER
      state = INSENSITIVE
      file = "progress-slider/slider-horiz-insens.png"
      border = { 5, 5, 3, 3 }
      stretch = TRUE
      orientation = HORIZONTAL
    }

# X Verticals

    image {
      function = SLIDER
      state = NORMAL
      file = "progress-slider/slider-vert.png" 
      border = { 3, 3, 5, 5 }
      stretch = TRUE
      orientation = VERTICAL
    }

    image {
      function = SLIDER
      state = ACTIVE
      file = "progress-slider/slider-vert-active.png" 
      border = { 3, 3, 5, 5 }
      stretch = TRUE
      orientation = VERTICAL
    }

    image {
      function = SLIDER
      state = PRELIGHT
      file = "progress-slider/slider-vert-prelight.png" 
      border = { 3, 3, 5, 5 }
      stretch = TRUE
      orientation = VERTICAL
    }

    image {
      function = SLIDER
      state = INSENSITIVE
      file = "progress-slider/slider-vert-insens.png"
      border = { 3, 3, 5, 5 }
      stretch = TRUE
      orientation = VERTICAL
    }
  }
}

